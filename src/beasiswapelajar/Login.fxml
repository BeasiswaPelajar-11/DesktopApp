      <TextField fx:id="txtpass" layoutX="243.0" layoutY="252.0" prefHeight="25.0" prefWidth="225.0" promptText="Enter Your Pasword" AnchorPane.leftAnchor="243.0" AnchorPane.rightAnchor="132.0" />
